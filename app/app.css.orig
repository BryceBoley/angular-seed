/*
 * Navbar effects
 */

nav ul li a {
    color: #478fa0;
    text-decoration: none;
    font-family: lato, arial, sans-serif;
}

nav ul li a:hover {
    text-decoration: none;
    /*background-color: #478fa0;*/
    color: #67abbb;
}

nav ul li a:link {
    text-decoration: none;
    list-style-type: none;
}

nav ul li a:focus {
    text-decoration: none;
    list-style-type: none;
    background-color: #478fa0;
    color: white;
}