/*
 * Media Queries
 */

@media screen and (max-width: 959px) {
    #container {
        width: 960px;
        margin: 0 auto;
    }

    #left-column {
        width: 700px;
        float: left;
        background: blue;
    }

    #right-column {
        width: 260px;
        float: left;
        background: orange;
    }
}



/*
 * spaces ng-view data from navbar
 */

.extra-space {
    margin-top: 25px;
}



